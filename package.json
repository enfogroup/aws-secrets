{
  "name": "@enfo/aws-secrets",
  "version": "0.0.1",
  "description": "Wrapper around AWS SSM",
  "main": "index.js",
  "scripts": {
    "build": "npx npm-run-all build:esbuild emit:types copy:files",
    "build:esbuild": "npx esbuild src/*.ts --outdir=.dist --format=cjs --minify",
    "emit:types": "tsc --emitDeclarationOnly --outDir .dist",
    "copy:files": "cp package.json .dist/ && cp README.MD .dist/ && cp LICENSE .dist/",
    "publish:package": "npm publish .dist",
    "test": "npx jest --runInBand",
    "lint": "npx eslint 'src/**/*.ts'",
    "lint:fix": "npx eslint -fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enfogroup/aws-secrets.git"
  },
  "keywords": [
    "aws",
    "ssm",
    "secrets"
  ],
  "author": "Urban Pettersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enfogroup/aws-secrets/issues"
  },
  "homepage": "https://github.com/enfogroup/aws-secrets#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "aws-sdk": "^2.997.0",
    "esbuild": "^0.12.28",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^4.3.8",
    "jest": "^27.2.5",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "peerDependencies": {
    "aws-sdk": "^2.991.0"
  },
  "dependencies": {
    "node-cache": "^5.1.2"
  }
}
