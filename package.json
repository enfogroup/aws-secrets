{
  "name": "@enfo/aws-secrets",
  "version": "1.3.1",
  "description": "NPM package for getting data from SSM, SecretsManager and KMS. All returned values are cached for further use.",
  "private": false,
  "main": "index.js",
  "scripts": {
    "build": "npx webpack",
    "publish:package": "npm publish .dist",
    "test": "npx jest --runInBand --config ./jest.config.js",
    "lint": "npx eslint 'src/**/*.ts'",
    "lint:fix": "npx eslint -fix 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enfogroup/aws-secrets.git"
  },
  "keywords": [
    "aws",
    "ssm",
    "kms",
    "secrets",
    "caching"
  ],
  "author": "Urban Pettersson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enfogroup/aws-secrets/issues"
  },
  "homepage": "https://github.com/enfogroup/aws-secrets#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.17",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "aws-sdk": "^2.1048.0",
    "copy-webpack-plugin": "^9.1.0",
    "eslint": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^4.3.8",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4",
    "typescript-transform-paths": "^3.3.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "peerDependencies": {
    "aws-sdk": "^2.991.0"
  },
  "dependencies": {
    "node-cache": "^5.1.2"
  }
}
